canvas=0
node-canvas=0
# Skip installing optional dependencies
ignore-optional=true
# Skip building native modules
build-from-source=false
# Prevent node-gyp usage
node-gyp=false
# Use legacy peer deps to fix compatibility issues
legacy-peer-deps=true
# Explicitly set canvas and node-canvas as optional
optional=canvas,node-canvas
# Set engine strict to false to avoid engine warnings
engine-strict=false 